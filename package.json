{
  "name": "devmod",
  "version": "1.0.0",
  "description": "A discord bot for moderating servers.",
  "homepage": "https://github.com/redxtech/devmod#readme",
  "bugs": "https://github.com/redxtech/devmod/issues",
  "license": "MIT",
  "author": "Gabe Dunn <gabed@hey.com>",
  "main": "src/main.ts",
  "repository": "github:redxtech/devmod",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node ./src/main.ts --strict",
    "lint": "NODE_OPTIONS=\"--no-warnings\" yarn xo --reporter stylish",
    "lint:fix": "NODE_OPTIONS=\"--no-warnings\" yarn xo --fix --reporter stylish"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-xo": "^0.32.1",
    "eslint-config-xo-typescript": "^0.32.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-ava": "^11.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^21.0.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2",
    "xo": "^0.33.0"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "docs"
    ]
  }
}
